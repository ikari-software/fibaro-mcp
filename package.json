{
  "name": "fibaro-mcp",
  "version": "1.0.0",
  "description": "Model Context Protocol server for Fibaro Home Center integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fibaro-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "start": "node dist/index.js",
