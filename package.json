    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "fibaro",
    "home-automation",
    "smart-home"
  ],
  "author": "Cezar \"ikari\" Pokorski <_@ikari.software>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikari-software/fibaro-mcp.git"
  },
  "homepage": "https://github.com/ikari-software/fibaro-mcp#readme",
  "bugs": {
    "url": "https://github.com/ikari-software/fibaro-mcp/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/nock": "^10.0.3",
    "@types/node": "^22.10.1",
    "@vitest/coverage-v8": "^4.0.16",
    "c8": "^10.1.3",
    "nock": "^14.0.10",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
